# mkinitcpio preset file for the 'kw' tool. This is the default

ALL_config="/etc/mkinitcpio.conf"
ALL_kver="/boot/vmlinuz-kw"

PRESETS=('default' 'fallback')

#default_config="/etc/mkinitcpio.conf"
default_image="/boot/initramfs-kw.img"
#default_options=""

#fallback_config="/etc/mkinitcpio.conf"
fallback_image="/boot/initramfs-kw-fallback.img"
fallback_options="-S autodetect"
